<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.faq.Faq">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAQ"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:gravity="center"
        android:fontFamily="@font/inder"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/faq1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title"
        android:text="How many items can i donate"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq1"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq1"
        app:layout_constraintTop_toTopOf="@id/faq1"/>
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq2"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq2"
        app:layout_constraintTop_toTopOf="@id/faq2"/>
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq3"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq3"
        app:layout_constraintTop_toTopOf="@id/faq3"/>
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq4"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq4"
        app:layout_constraintTop_toTopOf="@id/faq4"/>
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq5"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq5"
        app:layout_constraintTop_toTopOf="@id/faq5"/>
    <View
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:background="@drawable/ic_baseline_arrow_drop_down_24"
        app:layout_constraintEnd_toEndOf="@id/faq6"
        android:padding="10dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@id/faq6"
        app:layout_constraintTop_toTopOf="@id/faq6"/>
    <TextView
        android:id="@+id/faq2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/faq1"
        android:text="Who bares the delivery cost?"
        android:layout_margin="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/faq3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/faq2"
        android:text="What items can i donate?"
        android:layout_margin="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/faq4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/faq3"
        android:text="Can i donate from anywhere?"
        android:layout_margin="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/faq5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/faq4"
        android:text="Can i donate to charities"
        android:layout_margin="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/faq6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/faq5"
        android:text="Who gets the items i donate?"
        android:layout_margin="5dp"
        android:padding="10dp"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>